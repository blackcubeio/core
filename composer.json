{
    "name": "blackcube/core",
    "description": "Blackcube CMS Core",
    "keywords": ["blackcube", "cms", "core"],
    "homepage": "https://code.redcat.io/blackcube/core",
    "type": "yii2-extension",
    "license": "proprietary",
    "support": {
        "issues": "https://code.redcat.io/blackcube/core/issues",
        "wiki": "https://code.redcat.io/blackcube/core/wikis/home",
        "source": "https://code.redcat.io/blackcube/core/tree/devel"
    },
    "authors": [
        {
            "name": "Philippe Gaultier",
            "email": "pgaultier@redcat.io"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.org"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-fileinfo": "*",
        "ext-dom": "*",
        "ext-xmlreader": "*",
        "creocoder/yii2-flysystem": "~1.0",
        "hashids/hashids": "~4.1",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-cached-adapter": "~1.1",
        "swaggest/json-schema": "~0.12",
        "vlucas/phpdotenv": "~5.3",
        "yiisoft/yii2-imagine": "~2.3",
        "yiisoft/yii2": "~2.0"
    },
    "require-dev" : {
        "codeception/codeception": "~4.1",
        "codeception/module-yii2": "~1.1",
        "codeception/module-asserts": "~1.3",
        "yiisoft/yii2-debug":"~2.1",
        "yiisoft/yii2-gii": "~2.2"
    },
    "config": {
        "process-timeout": 1800,
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "autoload": {
        "psr-4": {
            "blackcube\\core\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests/",
            "tests\\_generated\\": "tests/_support/_generated/",
            "tests\\Helper\\": "tests/_support/Helper/"
        }
    },
    "scripts": {
        "post-install-cmd": [
        ]
    }
}
